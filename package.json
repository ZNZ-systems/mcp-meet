{
  "name": "mcp-meet",
  "version": "0.3.0",
  "description": "MCP server for one-click Google Meet scheduling and Apple Calendar mirroring",
  "type": "module",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-meet": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/znz-systems/mcp-meet.git"
  },
  "homepage": "https://github.com/znz-systems/mcp-meet#readme",
  "bugs": {
    "url": "https://github.com/znz-systems/mcp-meet/issues"
  },
  "author": "Panos Zepos <panos@znzsystems.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/index.ts",
    "cli": "tsx src/index.ts cli",
    "build": "tsc",
    "prepublishOnly": "pnpm run build && node scripts/make-cli-wrapper.mjs",
    "release": "npm version patch && git push origin main --tags"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "googleapis": "^139.0.0",
    "open": "^10.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}